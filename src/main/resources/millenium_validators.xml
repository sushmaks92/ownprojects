	<validator>
		<name>com.cerner.hi.pitc.validators.TechDesignCruLinkValidatorService</name>
		 <param key="pattern" value = "[\/]{2}crucible"/>
		  <param key="pattern1" value = "((T|t)ech\s?(D|d)oc)|((T|t)ech\s?(D|d)esign)|(DO_*)|((T|t)ech(D|d)oc)|((T|t)ech)"/>
